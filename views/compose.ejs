<%- include("partials/header") -%>

<h1> Compose </h1>
<br>
<form class="item" action="/compose" method="post">
    <div class="mb-3">
        <label for="postTitle" class="form-label">Title</label>
        <input type="Text" name="postTitle" placeholder="Add Post Title" class="form-control" id="post-tilte" >
    </div>
    <div class="mb-3">
        <label for="postBody" class="form-label">Body</label>
        <textarea name="postBody" placeholder="Add Post Body" class="form-control" id="post-body" rows="10"></textarea>
    </div>
    <button type="submit" name="list" class="btn btn-primary"> Publish</button>
</form>

<%- include("partials/footer") -%>